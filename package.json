{
  "name": "svelte-bootstrap-pug",
  "version": "2.0.0",
  "description": "Создание статических страниц с помощью технологий bootstrap, svelte, pug.",
  "main": "src/index.js",
  "scripts": {
    "dev": "rollup -c -w",
    "compile": "rollup -c",
    "pug-svg": "node scripts/sprite.js",
    "pug-init": "node scripts/pug.js",
    "index.html": "npm run pug-svg && rollup -c scripts/rollup.html.js && rm scripts/_html.js",
    "custom.css": "rollup -c scripts/rollup.css.js && rm docs/build/css.js*",
    "start": "sirv docs --no-clear"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yababay/svelte-bootstrap-pug.git"
  },
  "keywords": [
    "bootstrap",
    "svelte",
    "pug",
    "static",
    "svg",
    "html",
    "template"
  ],
  "author": "Михаил Беляков",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yababay/svelte-bootstrap-pug/issues"
  },
  "homepage": "https://github.com/yababay/svelte-bootstrap-pug#readme",
  "dependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-run": "^2.1.0",
    "@yababay67/pug-modules": "^0.0.6",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-pug": "^1.1.1",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^2.0.2"
  }
}
